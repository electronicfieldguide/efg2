    </target>       <target name="clean" depends="cleansrc">      
        <delete file="${efg.home}/lib/util/efgTemplate.jar" quiet="true"/>     </target>  
       <target name="cleansrc">
            <delete dir="templatesrc_compiled" quiet="true"/> 
            <delete dir="templatesrc" quiet="true"/>          
         </target>   
   </project>