				<EFGList resourceLink="" caption="" annotation="" efgKeyRef="1">gray-black bark</EFGList>
				<EFGList resourceLink="" caption="" annotation="" efgKeyRef="1">shallow ridges</EFGList>
				<EFGList resourceLink="" caption="" annotation="" efgKeyRef="1">large green-maroon buds</EFGList>
