<?xml version="1.0" encoding="UTF-8"?>
<TaxonPageTemplates xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="TaxonPageTemplate.xsd">
	<TaxonPageTemplate xslFileName="billFamilyTaxonPage.xsl" datasourceName="ithData">
		<groups>
			<!-- Header section -->
			<group rank="1" id="1">
				<characterValue rank="1" value="Genus"/>
				<characterValue rank="2" value="Species"/>
				<characterValue rank="3" value="Author"/>
			</group>
			<group rank="2" id="2">
				<characterValue rank="1" value="Family"/>
				<characterValue rank="2" value="Family common name"/>
			</group>
			<!-- Images section -->
			<group rank="3" id="3">
				<characterValue rank="1" label="Flower" value="Image fl lg"/>
				<characterValue rank="2" label="Fruit" value="Image fr lg"/>
				<characterValue rank="3" label="Fruit3" value="Flower"/>
				<characterValue rank="4" value="Flowering habit"/>
			</group>
			<!-- Identification section  -->
			<group rank="4" id="4">
				<characterValue rank="1" value="Identification"/>
			</group>
			<!-- Descriptions, Similar Species Section-->
			<group rank="5" id="5">
				<characterValue rank="1" value="Description"/>
				<characterValue rank="2" value="Local distribution"/>
				<characterValue rank="3" value="Habitat"/>
				<characterValue rank="4" value="Species range"/>
				<characterValue rank="5" value="Abundance"/>
				<characterValue rank="6" value="Phenology"/>
				<characterValue rank="7" value="Pollinators"/>
				<characterValue rank="8" value="Similar species"/>
				<characterValue rank="9" value="Comments"/>
				<characterValue rank="10" value="Herbivores"/>
			</group>
			<!-- Copyright -->
			<group rank="6" id="6" label="Credits">
				<characterValue rank="1" value="Credits"/>
			</group>
		</groups>
	</TaxonPageTemplate>
</TaxonPageTemplates>
