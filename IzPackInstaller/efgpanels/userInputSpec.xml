<userInput>
	<panel order="0">	
		<field type="search" variable="catalina_home">
		  <description align="left" 
		               txt="Select the path Tomcat 5.0.This is required by EFG2"
		               id="description.catalina_home"/>
		  <spec txt="Path to Tomcat:" checkfilename="bin/catalina.50.sh"
		        type="file" result="directory">
		  <choice value="/usr/lib/jakarta-tomcat" os="unix" />
		    </spec>
		</field>	
		<field type="search" variable="ImageMagick_home">
		  <description align="left" 
		               txt="Select the Path to Image Magick. This is required by EFG2"
		               id="description.ImageMagick_home"/>
		  <spec txt="Path to Image Magick:" checkfilename="convert"
		        type="file" result="directory">
		  <choice value="/usr/bin" os="unix" />
		   </spec>
		</field>
	</panel>
</userInput>